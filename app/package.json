{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .nuxt/ coverage/",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix && npm run lint:style -- --fix",
    "prepare": "cd .. && husky install app/.husky",
    "test": "TZ=utc jest --runInBand",
    "check": "npm run lint && npm run test",
    "jest-watch": "TZ=utc jest --watch"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/redirect-module": "^0.3.1",
    "@types/es6-promisify": "^6.0.1",
    "@types/luxon": "^3.2.0",
    "bootstrap": "^4.6.1",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.19.3",
    "deepmerge": "^4.2.2",
    "es6-promisify": "^7.0.0",
    "graphql": "^16.5.0",
    "hast-util-sanitize": "^3.0.2",
    "luxon": "^3.2.1",
    "nuxt": "^2.15.8",
    "nuxt-healthcheck": "^1.0.1",
    "nuxt-winston-log": "^1.2.0",
    "rehype": "^11.0.0",
    "rehype-raw": "^5.1.0",
    "rehype-sanitize": "^4.0.0",
    "rehype-stringify": "^8.0.0",
    "remark-gfm": "^1.0.0",
    "remark-highlight.js": "^6.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.1.0",
    "remark-slug": "^6.1.0",
    "remark-toc": "^7.2.0",
    "unified": "^9.2.2",
    "vfile": "^4.2.1",
    "vfile-reporter": "^6.0.2",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuex": "^3.6.2",
    "vuex-module-decorators": "^1.0.1",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.4.4",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "flush-promises": "^1.0.2",
    "husky": "^7.0.4",
    "jest": "^27.4.4",
    "jest-junit": "^14.0.0",
    "lint-staged": "^12.1.2",
    "mockdate": "^3.0.5",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "ts-jest": "^27.1.1",
    "vue-jest": "^3.0.4"
  }
}
