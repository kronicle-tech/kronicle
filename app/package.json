{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf .nuxt/ coverage/",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "TZ=utc jest",
    "jest-watch": "TZ=utc jest --watch"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jestSonar": {
    "reportPath": "build/test-results/jest",
    "reportFile": "jest-results.xml",
    "indent": 2
  },
  "dependencies": {
    "@nuxtjs/redirect-module": "^0.3.1",
    "bootstrap": "^5.1.1",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.18.0",
    "deepmerge": "^4.2.2",
    "hast-util-sanitize": "^4.0.0",
    "luxon": "^2.0.2",
    "nuxt": "^2.15.8",
    "nuxt-healthcheck": "^1.0.1",
    "nuxt-winston-log": "^1.2.0",
    "rehype-parse": "^8.0.3",
    "rehype-sanitize": "^5.0.0",
    "rehype-stringify": "^9.0.2",
    "remark-gfm": "^2.0.0",
    "remark-highlight.js": "^7.0.1",
    "remark-parse": "^10.0.0",
    "remark-preset-lint-recommended": "^6.1.0",
    "remark-slug": "^7.0.0",
    "remark-toc": "^8.0.0",
    "unified": "^10.1.0",
    "vfile": "^5.1.0",
    "vfile-reporter": "^7.0.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@vue/test-utils": "^1.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "flush-promises": "^1.0.2",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "lint-staged": "^11.1.2",
    "mockdate": "^3.0.5",
    "prettier": "^2.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "vue-jest": "^3.0.7"
  }
}
